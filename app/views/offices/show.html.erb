<% content_for :secondary_title do %>
  DCO Office at <%= @office.name %>
<% end %>

<% content_for :secondary_nav do %>
  <% if administrator? %>
    <%= link_to 'Edit', edit_office_path(@office) %>
    <%= link_to 'Delete', @office, method: :delete %>
  <% end %>
  <%= link_to 'Back', :back %>
<% end %>

<div>
  <span class="show-label">Name:</span>
  <p><%= @office.name %><p>
</div>
<div>
  <span class="show-label">Address:</span>
  <%= simple_format @office.address %>
</div>
<div>
  <span class="show-label">Created at:</span>
  <p><%= @office.created_at %></p>
</div>
<div>
  <span class="show-label">Updated at:</span>
  <p><%= @office.updated_at %></p>
</div>
